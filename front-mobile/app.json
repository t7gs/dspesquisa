{
  "expo": {
    "name": "GameSurvey",
    "slug": "front-mobile",
    "version": "1.0.0",
    "privacy":"public",
    "orientation": "portrait",
    "platforms":[
      "ios",
      "android"
    ],
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#455A64"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android":{
      "package":"com.wfrocha.gamesurvey"
    }
  }
}
